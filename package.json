{
  "name": "map-gl-compass-pro",
  "version": "0.0.9",
  "description": "Compass pro for react-map-gl based projects",
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "rimraf dist && npx tsc && node ./tools/copyCss",
    "build:watch": "npx tsc --watch & node ./tools/copyCss --watch",
    "lint": "eslint 'src/**/*.{ts,tsx}' --max-warnings=0",
    "publish": "npm run build && npm version patch -m \"bump version\" && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jedluk/map-gl-compass-pro"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "bugs": {
    "url": "https://github.com/jedluk/map-gl-compass-pro/issues"
  },
  "peerDependencies": {
    "react-map-gl": ">=6.0.0"
  },
  "keywords": [
    "react-map-gl",
    "compass",
    "map-gl",
    "maplibre",
    "mapbox"
  ],
  "author": {
    "name": "Jędrzej Łukasiuk",
    "email": "jedrzej.lukasiuk@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.41.0",
    "react-map-gl": "^7.0.23",
    "rimraf": "^5.0.1",
    "typescript": "^5.0.4"
  }
}
