{
  "name": "map-gl-compass-pro",
  "version": "1.0.0",
  "description": "Compass pro for react-map-gl based projects",
  "main": "dist/Compass.js",
  "scripts": {
    "build": "rimraf dist && npx tsc && cp -r src/style dist/style",
    "build:watch": "rimraf dist && mkdir dist && cp src/style/*.css dist/style && npx tsc --watch",
    "lint": "eslint 'src/**/*.{ts,tsx}' --max-warnings=0"
  },
  "peerDependencies": {
    "react-map-gl": ">=6.0.0"
  },
  "keywords": [
    "react-map-gl",
    "compass",
    "map-gl",
    "maplibre",
    "mapbox"
  ],
  "author": {
    "name": "Jędrzej Łukasiuk",
    "email": "jedrzej.lukasiuk@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.41.0",
    "react-map-gl": "^7.0.23",
    "rimraf": "^5.0.1",
    "typescript": "^5.0.4"
  }
}
